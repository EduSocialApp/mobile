{
    "cli": {
        "version": ">= 12.5.1",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development",
            "environment": "development",
            "ios": {
                "simulator": true
            },
            "android": {
                "buildType": "apk"
            }
        },
        "preview": {
            "distribution": "internal",
            "channel": "preview",
            "environment": "preview",
            "ios": {
                "simulator": true
            },
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "https://edusocial.felipesobral.com.br"
            }
        },
        "production": {
            "distribution": "store",
            "channel": "production",
            "environment": "production",
            "ios": {
                "simulator": false
            },
            "android": {
                "buildType": "app-bundle"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "https://edusocial.felipesobral.com.br"
            }
        }
    },
    "submit": {
        "production": {}
    }
}